#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nhinlechi/Documents/Repos/training/system-design-practice/projects/clinic_appointment_system/src/node_modules/.pnpm/module-lookup-amd@9.0.5/node_modules/module-lookup-amd/bin/node_modules:/Users/nhinlechi/Documents/Repos/training/system-design-practice/projects/clinic_appointment_system/src/node_modules/.pnpm/module-lookup-amd@9.0.5/node_modules/module-lookup-amd/node_modules:/Users/nhinlechi/Documents/Repos/training/system-design-practice/projects/clinic_appointment_system/src/node_modules/.pnpm/module-lookup-amd@9.0.5/node_modules:/Users/nhinlechi/Documents/Repos/training/system-design-practice/projects/clinic_appointment_system/src/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/nhinlechi/Documents/Repos/training/system-design-practice/projects/clinic_appointment_system/src/node_modules/.pnpm/module-lookup-amd@9.0.5/node_modules/module-lookup-amd/bin/node_modules:/Users/nhinlechi/Documents/Repos/training/system-design-practice/projects/clinic_appointment_system/src/node_modules/.pnpm/module-lookup-amd@9.0.5/node_modules/module-lookup-amd/node_modules:/Users/nhinlechi/Documents/Repos/training/system-design-practice/projects/clinic_appointment_system/src/node_modules/.pnpm/module-lookup-amd@9.0.5/node_modules:/Users/nhinlechi/Documents/Repos/training/system-design-practice/projects/clinic_appointment_system/src/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
